active_configuration = 'default'

################################################################################
#                                [cfg: default]                                #
################################################################################
[cfg: default]
  config_icon = 'icons/board/nucleo.png'
  
  # 1. Version 
  project_type = 'makefile'
  project_version = '7'
  
  # 2. Chip 
  chip_name = 'stm32f303k8'
  
  # 3. Board 
  board_name = 'nucleo-f303k8'
  
  # 4. Probe 
  probe_name         = 'stlink-v2-1'
  transport_protocol = 'hla-swd' # Protocol for communication between Probe and Microcontroller.
  
  # 5. Project Layout
  BUILD_DIR            = '<project>/build'
  BIN_FILE             = '<project>/build/application.bin'
  ELF_FILE             = '<project>/build/application.elf'
  BOOTLOADER_FILE      = 'None'
  BOOTSWITCH_FILE      = 'None'
  PARTITIONS_CSV_FILE  = 'None'
  LINKERSCRIPT         = '<project>/config/linkerscript.ld'
  MAKEFILE             = '<project>/config/makefile'
  GDB_FLASHFILE        = 'None'
  OPENOCD_CHIPFILE     = '<project>/config/openocd_chip.cfg'
  OPENOCD_PROBEFILE    = '<project>/config/openocd_probe.cfg'
  PYOCD_FILE           = 'None'
  PACKPATH             = 'None'
  
  # 6. Tools 
  COMPILER_TOOLCHAIN   = 'gnu_arm_toolchain_10.3.1_20210824_32b'
  BUILD_AUTOMATION     = 'gnu_make_4.2.1_64b'
  FLASHTOOL            = 'openocd_0.12.0_dev01004_64b'

